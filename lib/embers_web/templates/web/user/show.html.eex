<%= EmbersWeb.LayoutView.sidebar assigns %>
<div id="view" view="user" class="has-sidebar">
  <%= render_shared("top_bar.html") %>
  <%= render("_profile_header.html", assigns) %>
  <main class="padded columns-layout">
    <div class="main-content">
      <div class="feed">
        <%= for activity <- @activities.entries do %>
          <%= render(EmbersWeb.Web.PostView, "post.html", conn: @conn, post: activity) %>
        <% end %>
      </div>
    </div>
    <aside></aside>
  </main>
</div>
